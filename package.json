{
  "name": "apm",
  "version": "4.0.0-alpha.0",
  "private": true,
  "description": "A package manager for AviUtl",
  "keywords": [],
  "homepage": "https://github.com/team-apm/apm#readme",
  "bugs": {
    "url": "https://github.com/team-apm/apm/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/team-apm/apm.git"
  },
  "license": "MIT",
  "author": "ato lash <28973861+hal-shu-sato@users.noreply.github.com>",
  "main": ".webpack/main",
  "scripts": {
    "cm": "cz",
    "fix": "run-s -c fix:eslint fix:prettier",
    "fix:eslint": "eslint --fix .",
    "fix:prettier": "prettier --write .",
    "lint": "run-s -c lint:ts lint:eslint lint:prettier",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "lint:ts": "tsc --noEmit",
    "make": "yarn run notices && electron-forge make",
    "notices": "yarn licenses generate-disclaimer --frozen-lockfile --prod --ignore-platform --ignore-engines --ignore-optional > ThirdPartyNotices.txt",
    "package": "yarn run notices && electron-forge package",
    "prepare": "husky",
    "publish": "yarn run notices && electron-forge publish",
    "release": "release-it",
    "release:major": "release-it major",
    "release:minor": "release-it minor",
    "release:patch": "release-it patch",
    "release:prerelease": "release-it prerelease",
    "start": "electron-forge start"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "electron-debug": "^4.1.0",
    "electron-is-dev": "^3.0.1",
    "electron-log": "^5.4.1",
    "electron-squirrel-startup": "^1.0.1",
    "electron-store": "^8.2.0",
    "electron-window-state": "^5.0.3",
    "fs-extra": "^11.3.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "update-electron-app": "^3.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-rpm": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron-forge/plugin-webpack": "^7.8.1",
    "@electron-forge/publisher-github": "^7.8.1",
    "@electron/fuses": "^1.8.0",
    "@eslint/js": "^9.31.0",
    "@release-it/conventional-changelog": "^10.0.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vercel/webpack-asset-relocator-loader": "1.7.3",
    "commitizen": "^4.3.1",
    "css-loader": "^7.1.2",
    "cz-conventional-changelog": "^3.3.0",
    "electron": "37.2.1",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "node-loader": "^2.1.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.6.2",
    "prettier-plugin-packagejson": "^2.5.18",
    "release-it": "^19.0.3",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.36.0"
  },
  "packageManager": "yarn@1.22.22",
  "productName": "AviUtl Package Manager"
}
